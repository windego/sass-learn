{
  "name": "sass-learn",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "sass --watch scss:css",
    "watch": "sass --watch src/scss:dist/css",
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html --dist-dir www",
    "lint": "stylelint '**/*.scss'",
    "fix": "stylelint --fix \"**/*.{,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">12"
  },
  "dependencies": {
    "parcel": "^2.6.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.6.2",
    "postcss-scss": "^4.0.4",
    "sass": "^1.53.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0"
  }
}
